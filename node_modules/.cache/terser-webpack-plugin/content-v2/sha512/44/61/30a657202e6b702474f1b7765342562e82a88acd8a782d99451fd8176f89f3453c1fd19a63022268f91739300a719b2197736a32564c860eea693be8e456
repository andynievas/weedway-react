os\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"input\",{type:\"text\",placeholder:\"Buscar\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"button\",{children:\"Nuevo Post\"})]});}}]);return Cuenta;}(react_default.a.Component);/* harmony default export */ var cuenta = (cuenta_Cuenta);\n// CONCATENATED MODULE: ./src/Components/modalbody.js\nvar modalbody_ModalBody=/*#__PURE__*/function(_React$Component){Object(inherits[\"a\" /* default */])(ModalBody,_React$Component);var _super=Object(createSuper[\"a\" /* default */])(ModalBody);function ModalBody(){var _this;Object(classCallCheck[\"a\" /* default */])(this,ModalBody);for(var _len=arguments.length,args=new Array(_len),_key=0;_key<_len;_key++){args[_key]=arguments[_key];}_this=_super.call.apply(_super,[this].concat(args));_this.styles={margin:'auto',padding:'0 20px',color:'rgb(220,220,220)',background:'rgb(60,60,60)',borderRadius:'20px 20px 0 0',marginTop:'70px'};_this.buttonStyle={margin:'0px',width:'50px',height:'50px',backgroundColor:'red',fontSize:'28px'};return _this;}Object(createClass[\"a\" /* default */])(ModalBody,[{key:\"render\",value:function render(){var _this2=this;if(this.props.view==='Mensajes'){return/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{style:this.styles,children:[/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{style:{display:'flex'},children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"button\",{style:this.buttonStyle,onClick:function onClick(){_this2.props.setEstado();},children:\" \\xD7\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"p\",{style:{width:'100%',textAlign:'center',height:'18px'},children:this.props.view})]}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(mensajes,{})]});}else if(this.props.view==='Cuenta'){return/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{style:this.styles,children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"button\",{style:this.buttonStyle,onClick:function onClick(){_this2.props.setEstado();},children:\" \\xD7\"}),/*#__PURE__*/Object(jsx_run